<zk xmlns:n="native" xmlns:c="client"> 
	<window id="windowProjetoBase" class="page portal" apply="com.agfgerador.compartilhado.controller.DashboardController" > 
		<div class="topo"> 
			<div class="nome-cliente" >
				<label id="nomeInstituicao"></label>
			</div> 
			<div class="right-menu">
			
						 <div class="divbandboxbusca"> 
